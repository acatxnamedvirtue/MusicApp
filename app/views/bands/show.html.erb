<h1><%= @band.name %></h1>
<%= link_to "Edit #{@band.name}", edit_band_url(@band) %>
<%= link_to "Delete #{@band.name}", @band, method: :delete %>

<h2>Albums</h2>
<%= link_to "Add an Album", new_band_album_url(@band) %>
<br>
<br>

<% @band.albums.each do |album| %>
  <%= link_to "#{album.title}", album_url(album) %>
<% end %>
